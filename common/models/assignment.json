{
  "name": "assignment",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "assignmentName": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "isOfficial": {
      "type": "boolean",
      "default": false
    },
    "avatar": {
      "type": "string"
    },
	"create_time":{
		"type":"date"
	},
	"priority":{
		"type":"number"
	}
  },
  "validations": [],
  "relations": {
      "flowto": {
        "type": "hasMany",
        "model": "flowto",
        "foreignKey": "assignmentId"
      },
      "flowtoUser": {
        "type": "belongsTo",
        "model": "flowtoUser",
        "foreignKey": ""
      }
  },  
  "acls": [
	  {
	        "accessType": "WRITE",
	        "principalType": "ROLE",
	        "principalId": "$everyone",
	        "permission": "DENY",
	        "property": "destroyById"
	      },
	  {
	        "accessType": "WRITE",
	        "principalType": "ROLE",
	        "principalId": "$owner",
	        "permission": "ALLOW",
	        "property": "destroyById"
	      }
		  
	
  ],
  "methods": {}
}
